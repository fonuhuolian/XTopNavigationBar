<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="XTopNavigationBar">

        <!--TopBar样式-->
        <attr name="xBar_style" format="enum">
            <enum name="titleLeft" value="1" />
            <enum name="titleMiddle" value="2" />
        </attr>

        <attr name="xBar_right_btn_show" format="flags">
            <flag name="left" value="1" />
            <flag name="middle" value="10" />
            <flag name="right" value="100" />
        </attr>

    </declare-styleable>

</resources>